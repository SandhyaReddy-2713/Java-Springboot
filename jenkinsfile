pipeline {
    agent any
    }
    stages {
        stage('git checkout') {
            steps {
                git 'https://github.com/SandhyaReddy-2713/Java-Springboot.git'
            }
        }
        stage('build') {
            steps {
                sh 'mvn clean install'
            }
        }
        stage('test') {
            steps {
                echo 'this is test phase'
            }
        }
        stage('deploy') {
            steps {
                echo 'this is deploy phase'
            }
        }
    }
}

       
